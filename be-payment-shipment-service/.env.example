# Server Configuration
NODE_ENV=production
PORT=3000

# API Security
SERVICE_API_KEY=your-secret-api-key-here

# CORS Configuration (tambahkan semua domain yang diizinkan)
ALLOWED_ORIGINS=https://regalpaw.id,https://www.regalpaw.id,https://dev.regalpaw.id

# Payment Gateway - Xendit
XENDIT_SECRET_KEY=xnd_production_your_secret_key
